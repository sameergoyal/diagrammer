{
  "name": "diagrammer",
  "description": "A library to display an interactive editor for any graph-like data.",
  "version": "1.0.3",
  "author": {
    "name": "Sameer Goyal"
  },
  "homepage": "https://sameergoyal.github.io/diagrammer",
  "contributors": [
    "Sameer Goyal"
  ],
  "engines": {
    "node": ">=16"
  },
  "main": "dist/diagrammer.js",
  "types": "dist/diagrammer.d.ts",
  "typings": "dist/diagrammer.d.ts",
  "license": "Apache-2.0",
  "keywords": [
    "visualizaton",
    "diagram",
    "interactive",
    "canvas",
    "editor",
    "drag",
    "drop",
    "workflow",
    "state-machine",
    "flowchart",
    "diagram-maker",
    "diagrammer"
  ],
  "devDependencies": {
    "@cypress/code-coverage": "^3.11.0",
    "@storybook/addon-essentials": "^7.1.1",
    "@storybook/addon-storysource": "^7.1.1",
    "@storybook/html": "^7.1.1",
    "@storybook/html-webpack5": "^7.1.1",
    "@storybook/theming": "^7.1.1",
    "@types/dagre": "^0.7.49",
    "@types/enzyme": "^3.10.13",
    "@types/jest": "^29.5.3",
    "@types/lodash-es": "^4.17.8",
    "@types/uuid": "^9.0.2",
    "@types/webpack": "^5.28.1",
    "@types/webpack-env": "^1.18.1",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "autoprefixer": "^10.4.14",
    "babel-loader": "^9.1.3",
    "babel-plugin-istanbul": "^6.1.1",
    "bundle-declarations-webpack-plugin": "^4.0.1",
    "classnames": "^2.3.2",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "cypress": "^12.17.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-preact-pure": "^4.1.0",
    "enzyme-to-json": "^3.6.2",
    "es6-shim": "^0.35.8",
    "eslint": "^8.46.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.13",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "html-webpack-plugin": "^5.5.3",
    "immer": "^10.0.2",
    "jest": "^29.6.2",
    "jest-environment-jsdom": "^29.6.2",
    "jest-snapshot": "^29.6.2",
    "lodash-es": "^4.17.21",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss": "^8.4.27",
    "postcss-loader": "^7.3.3",
    "postcss-scss": "^4.0.6",
    "preact": "^10.16.0",
    "react-redux": "^8.1.2",
    "redux": "^4.2.1",
    "redux-undo-redo": "^2.1.1",
    "sass": "^1.64.1",
    "sass-loader": "^13.3.2",
    "storybook": "^7.1.1",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "typedoc": "^0.24.8",
    "typedoc-plugin-missing-exports": "^2.0.0",
    "typescript": "^5.1.6",
    "uuid": "^9.0.0",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-merge": "^5.9.0"
  },
  "optionalDependencies": {
    "dagre": "^0.8.5"
  },
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "clean": "rm -rf ./coverage && rm -rf ./dist && rm -rf ./docs",
    "documentation": "typedoc --plugin typedoc-plugin-missing-exports",
    "start": "storybook dev -p 31213",
    "server": "npm run start",
    "lint": "eslint --ext .js,.ts,.tsx src/",
    "lint:fix": "eslint --ext .js,.ts,.tsx src/ --fix",
    "test": "tsc --noEmit -p . && npx jest --config jest.json",
    "test:dev": "tsc --noEmit -p . && npx jest --config jest.json --watch",
    "test:update": "tsc --noEmit -p . && npx jest --config jest.json --updateSnapshot",
    "release": "npm run lint && npm run test && npm run build && npm run release:integ && npm run documentation",
    "build:integ": "storybook build -o docs/",
    "lint:integ": "npx eslint --ext .js,.ts,.tsx integ/ -c integ/.eslintrc && eslint --ext .js,.ts,.tsx cypress/e2e/ -c cypress/.eslintrc",
    "lint:integ:fix": "npx eslint --ext .js,.ts,.tsx integ/ -c integ/.eslintrc --fix && eslint --ext .js,.ts,.tsx cypress/e2e/ -c cypress/.eslintrc --fix",
    "test:integ": "npm run build:integ && tsc --noEmit -p ./cypress && cypress run --env coverage=true",
    "test:integ:dev": "tsc --noEmit -p ./cypress && cypress open --config baseUrl=http://localhost:31213 --env coverage=true",
    "release:integ": "npm run lint:integ && npm run test:integ",
    "prepublishOnly": "npm run clean && npm run release"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/sameergoyal/diagrammer"
  }
}
