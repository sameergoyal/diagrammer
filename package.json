  "author": {
    "name": "Sameer Goyal"
  },
  "homepage": "https://sameergoyal.github.io/diagrammer",
  "contributors": [
    "Sameer Goyal"
  ],
  "engines": {
    "node": ">=16"
  },
  "main": "dist/diagrammer.js",
  "types": "dist/diagrammer.d.ts",
  "typings": "dist/diagrammer.d.ts",
  "license": "Apache-2.0",
  "keywords": [
    "visualizaton",
    "diagram",
    "interactive",
    "canvas",
    "editor",
    "drag",
    "drop",
    "workflow",
    "state-machine",
    "flowchart",
    "diagram-maker",
    "diagrammer"
  ],
  "devDependencies": {
    "@cypress/code-coverage": "^3.12.44",
