    "dagre": "^0.8.5"
  },
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "clean": "rm -rf ./coverage && rm -rf ./dist && rm -rf ./docs",
    "documentation": "typedoc --plugin typedoc-plugin-missing-exports",
    "start": "storybook dev -p 31213",
    "server": "npm run start",
    "lint": "eslint --ext .js,.ts,.tsx src/",
    "lint:fix": "eslint --ext .js,.ts,.tsx src/ --fix",
    "test": "tsc --noEmit -p . && npx jest --config jest.json",
    "test:dev": "tsc --noEmit -p . && npx jest --config jest.json --watch",
    "test:update": "tsc --noEmit -p . && npx jest --config jest.json --updateSnapshot",
    "release": "npm run lint && npm run test && npm run build && npm run release:integ && npm run documentation",
    "build:integ": "storybook build -o docs/",
    "lint:integ": "npx eslint --ext .js,.ts,.tsx integ/ -c integ/.eslintrc && eslint --ext .js,.ts,.tsx cypress/e2e/ -c cypress/.eslintrc",
    "lint:integ:fix": "npx eslint --ext .js,.ts,.tsx integ/ -c integ/.eslintrc --fix && eslint --ext .js,.ts,.tsx cypress/e2e/ -c cypress/.eslintrc --fix",
    "test:integ": "npm run build:integ && tsc --noEmit -p ./cypress && cypress run --env coverage=true",
    "test:integ:dev": "tsc --noEmit -p ./cypress && cypress open --config baseUrl=http://localhost:31213 --env coverage=true",
    "release:integ": "npm run lint:integ && npm run test:integ",
    "prepublishOnly": "npm run clean && npm run release"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/sameergoyal/diagrammer"
  },
  "packageManager": "yarn@1.22.19+sha1.4ba7fc5c6e704fce2066ecbfb0b0d8976fe62447"
}
